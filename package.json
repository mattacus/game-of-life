{
  "name": "game-of-life",
  "version": "1.0.0",
  "description": "Conway's Game of Life, React + Canvas",
  "main": "index.js",
  "scripts": {
    "build:prod": "parcel build src/index.html",
    "build:docker": "docker build -t mattld/react-gol .",
